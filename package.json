{
  "name": "Fevre",
  "version": "1.0.0",
  "description": "Site da Fundação Educacional de Volta Redonda",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass -w src/scss/main.scss dist/css/style.css",
    "prefix:css": "postcss dist/css/style.css --no-map --config postcss.config.js -o dist/css/style.prefix.css",
    "compress:css": "node-sass --output-style compressed dist/css/style.prefix.css dist/css/style.min.css",
    "build:css": "npm-run-all prefix:css compress:css",
    "build:js": "uglifyjs src/js/*.js -m -c -o dist/js/scripts.min.js"
  },
  "author": "Golveira",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^9.6.1",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.3",
    "uglify-js": "^3.6.0"
  }
}
